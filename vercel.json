{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "dest": "/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(html|css|js|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/public/$1"
    },
    {
      "src": "/resultados",
      "dest": "/public/resultados.html"
    },
    {
      "src": "/configuracoes",
      "dest": "/public/configuracoes.html"
    },
    {
      "src": "/estatisticas",
      "dest": "/public/estatisticas.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}

